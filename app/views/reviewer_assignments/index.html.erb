<p id="notice"><%= notice %></p>

<h1>Abstract Reviewer Assignments</h1>

<table>
  <thead>
    <tr>
      <th>Abstract</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @abstract_reviewer_assignments.each do |abstract_reviewer_assignment| %>
      <tr>
        <td><%= abstract_reviewer_assignment.abstract_id %></td>
        <td><%= abstract_reviewer_assignment.user_id %></td>
        <td><%= link_to 'Show', abstract_reviewer_assignment %></td>
        <td><%= link_to 'Edit', edit_abstract_reviewer_assignment_path(abstract_reviewer_assignment) %></td>
        <td><%= link_to 'Destroy', abstract_reviewer_assignment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Abstract Reviewer Assignment', new_abstract_reviewer_assignment_path %>
